    "antlr-qm": "cd src/antlr4 && antlr4ts -o generated QueryModification.g4",
    "test": "ts-mocha src/_t/*.ts src/**/_t/*.ts",
    "testOnly": "ts-mocha"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.11",
