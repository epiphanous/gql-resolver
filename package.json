    "fetch-sparql-endpoint": "^1.4.0",
    "funfix": "^7.0.1",
    "id64": "^1.0.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "n3": "^1.1.1",
    "node-fetch": "^2.5.0",
    "object-sizeof": "^1.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
